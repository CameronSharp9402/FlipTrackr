<Window x:Class="CSharpResaleBusinessTracker.ManageCategoriesDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpResaleBusinessTracker"
        mc:Ignorable="d"
        Title="ManageCategoriesDialog" Height="300" Width="300" WindowStartupLocation="CenterOwner">
    <Grid Margin="10">
        <ListBox x:Name="CategoryList" />
        <Button Content="Delete Selected" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,10,0,0" Click="DeleteSelected_Click" />
    </Grid>
</Window>
